{
    "extends": "../../../.eslintrc.json",
    "env": {
        "browser": true,
        "es6": false,
        "node": false
    },
    "parserOptions": {
        "sourceType": "script"
    },
    "rules": {
        "no-var": "off",
        "object-shorthand": "off",
        "prefer-arrow-callback": "off",
        "prefer-template": "off"
    }
}
