{
  "name": "bootstrapcdn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "del-cli -f \"public/assets/js/bundle.dev.js\" \"public/assets/js/main.dev.js\"",
    "babel": "babel public/assets/js/main.js",
    "critical": "wait-on tcp:3339 && critical http://127.0.0.1:3339/ --base public --assetPaths public --ignore \"@import\" --ignore \":root\" > public/assets/css/critical.css",
    "critical-gen": "cross-env GENERATE_CRITICAL_CSS=true PORT=3339 npm-run-all --race --parallel start-node critical",
    "build": "npm-run-all \"babel -- -o public/assets/js/main.prod.js\" critical-gen",
    "postbuild": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --output public/assets/js/bundle.prod.js public/assets/js/vendor/clipboard.min.js public/assets/js/main.prod.js",
    "build:dev": "npm-run-all clean \"babel -- -o public/assets/js/main.dev.js\" critical-gen",
    "postbuild:dev": "shx cat public/assets/js/vendor/clipboard.min.js public/assets/js/main.dev.js > public/assets/js/bundle.dev.js",
    "eslint": "eslint .",
    "puglint": "pug-lint .",
    "lint": "npm-run-all --parallel eslint puglint",
    "mocha": "mocha --timeout 15000 --slow 500 --exit -i -g \"functional\" -R dot ./tests/",
    "mocha:suite": "mocha --timeout 15000 --slow 500 --exit -R dot ./tests/",
    "mocha:functional": "mocha --timeout 15000 --slow 500 --exit -R tap ./tests/functional_test.js",
    "dev": "npm run watch",
    "watch": "npm-run-all --parallel watch:*",
    "watch:cssjs": "nodemon --ext css,js --ignore tests --ignore public/assets/css/critical.css --ignore public/assets/js/main.dev.js --ignore public/assets/js/bundle.dev.js --exec \"npm run build:dev\"",
    "watch:other": "nodemon app.js --ext pug,svg,png,yml --ignore tests",
    "start-node": "node app.js",
    "start": "npm run build && cross-env NODE_ENV=production node app.js",
    "stop": "forever stop app.js",
    "restart": "npm run stop & npm run start",
    "travis": "npm-run-all build --parallel lint mocha:suite",
    "test": "npm-run-all build --parallel lint mocha",
    "purge-latest": "bash ./scripts/purge.sh",
    "bootswatch": "node ./scripts/bootswatch.js",
    "integrity": "node ./scripts/integrity.js",
    "wp-plugin": "node ./scripts/wp-plugin.js"
  },
  "dependencies": {
    "async": "^2.6.0",
    "compression": "^1.7.2",
    "cross-env": "^5.1.4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-sitemap": "^1.8.0",
    "express-sslify": "^1.2.0",
    "forever": "^0.15.3",
    "helmet": "^3.12.0",
    "js-yaml": "^3.11.0",
    "jstransformer-clean-css": "^2.1.0",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
    "rollbar": "^2.3.9",
    "semver": "^5.5.0",
    "serve-favicon": "^2.4.5",
    "serve-static": "^1.13.2",
    "sri-toolbox": "^0.2.0",
    "staticify": "^2.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-eslint": "^8.2.2",
    "critical": "^1.2.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "format": "^0.2.2",
    "fs-walk": "^0.0.2",
    "htmlencode": "^0.0.4",
    "html-validator": "^2.2.3",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "pug-lint": "^2.5.0",
    "pug-lint-config-clock": "^2.0.0",
    "request": "^2.85.0",
    "shx": "^0.2.2",
    "uglify-js": "^3.3.16",
    "wait-on": "^2.1.0"
  },
  "engines": {
    "node": "8.x"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  }
}
