{
  "name": "bootstrapcdn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "snyk-protect": "snyk protect",
    "test": "node make test"
  },
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "async": "^2.6.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "compression": "^1.7.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-sitemap": "^1.8.0",
    "express-sslify": "^1.2.0",
    "helmet": "^3.9.0",
    "js-yaml": "^3.10.0",
    "jstransformer-babel": "^2.1.0",
    "jstransformer-clean-css": "^2.0.0",
    "jstransformer-uglify-js": "^1.2.0",
    "morgan": "^1.9.0",
    "pug": "^2.0.0-rc.4",
    "rollbar": "^2.3.3",
    "serve-favicon": "^2.4.5",
    "serve-static": "^1.13.1",
    "shelljs": "^0.5.3",
    "snyk": "^1.59.1",
    "sri-toolbox": "^0.2.0",
    "uglify-js": "^2.8.29",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "bootlint": "^0.14.2",
    "forever": "^0.15.3",
    "format": "^0.2.2",
    "fs-walk": "^0.0.1",
    "html-validator": "^2.2.3",
    "htmlencode": "^0.0.4",
    "mocha": "^4.0.1",
    "pug-lint": "^2.5.0",
    "pug-lint-config-clock": "^2.0.0",
    "request": "^2.83.0",
    "semver": "^5.4.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "space": 4,
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "capitalized-comments": "off",
      "global-require": "error",
      "import/no-unassigned-import": [
        "error",
        {
          "allow": [
            "app.js",
            "shelljs/make"
          ]
        }
      ],
      "init-declarations": [
        "error",
        "always"
      ],
      "new-cap": "off",
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "Property": true,
            "VariableDeclarator": true
          }
        }
      ],
      "no-undefined": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "quote-props": [
        "error",
        "consistent"
      ],
      "unicorn/filename-case": "off",
      "unicorn/no-process-exit": "off"
    },
    "overrides": [
      {
        "files": "public/assets/js/main.js",
        "envs": [
          "browser"
        ],
        "rules": {
          "unicorn/no-abusive-eslint-disable": "off"
        }
      },
      {
        "files": "tests/*.js",
        "envs": [
          "mocha"
        ]
      }
    ]
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "snyk": true
}
